{
	"env": {
		"browser": true,
		"es2021": true
	},
	"parser": "@babel/eslint-parser",
	"extends": [
		"airbnb",
		"plugin:react/recommended",
		"eslint:recommended",
		"plugin:prettier/recommended"
	],
	"parserOptions": {
		"ecmaFeatures": {
			"jsx": true
		},
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"plugins": ["react", "prettier"],
	"rules": {}
}
